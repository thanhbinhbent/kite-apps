{
  "name": "kite-app",
  "version": "0.0.1",
  "scripts": {
    "build": "set NODE_ENV=githubPage && rspack build",
    "build:dev": "set NODE_ENV=development && rspack build",
    "build:git": "set NODE_ENV=development && rspack build",
    "build:start": "cd dist && rspack serve",
    "start:dev": "set NODE_ENV=development && rspack serve",
    "start:git": "set NODE_ENV=githubPage && rspack serve",
    "deploy:git": "set NODE_ENV=githubPage & rspack build && echo > dist/.nojekyll && gh-pages -d dist"
  },
  "devDependencies": {
    "@rspack/cli": "~1.2.0",
    "@rspack/core": "~1.2.0",
    "@rspack/plugin-react-refresh": "~1.0.1",
    "@tailwindcss/postcss": "^4.0.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.1.0",
    "css-loader": "^7.1.2",
    "gh-pages": "^6.3.0",
    "postcss": "^8.2.1",
    "postcss-loader": "^8.0.0",
    "react-refresh": "^0.14.0",
    "tailwindcss": "^4.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@module-federation/enhanced": "^0.8.9",
    "dotenv": "^16.4.7",
    "kite-app": "file:",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.3.0"
  }
}
